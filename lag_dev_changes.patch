+        case 'RAISE_HAND_NOTIFICATION':
+            return `${title ?? titleKey}${i18next.t(descriptionKey ?? '')}`;
+        default:
+            return `${i18next.t(titleKey ?? '')}. ${i18next.t(descriptionKey ?? '')}.`;
+        }
+    }
