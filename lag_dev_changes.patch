+                            {this._renderContent()}
+                        </FocusOn>
+                    </AccessibleDialogPortal>
+
+                )}
+            </div>
+        );
+    }
+
+    /**
+     * Sets the context menu dialog style for positioning it on screen.
+     *
+     * @param {DOMRectReadOnly} size -The size info of the current context menu.
+     *
+     * @returns {void}
+     */
