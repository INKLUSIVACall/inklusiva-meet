+
+export const ASSISTANCE_BREAKOUT_ROOM_HANDLED = 'ASSISTANCE_BREAKOUT_ROOM_HANDLED';
+export const ASSISTANCE_BREAKOUT_ROOM_UNHANDLED = 'ASSISTANCE_BREAKOUT_ROOM_UNHANDLED';
